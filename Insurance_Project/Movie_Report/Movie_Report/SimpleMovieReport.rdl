  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Movies1</DataSourceName>
        <CommandText>SELECT       Director.FullName, Genre.Genre, Certificate.Certificate, Film.Title, Film.ReleaseDate, Film.RunTimeMinutes, Film.OscarWins
FROM      Certificate INNER JOIN
                   Film ON Certificate.CertificateID=Film.CertificateID INNER JOIN
                  Director ON Film.DirectorID = Director.DirectorID INNER JOIN
                  Genre ON Film.GenreID = Genre.GenreID </CommandText>
      </Query>
